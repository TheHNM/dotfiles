#!/bin/sh

PATH=$PATH:$HOME/.local/bin/; export PATH
export QT_QPA_PLATFORMTHEME="qt5ct"
export EDITOR="nvim"
export BROWSER="firefox"
export TERMINAL="termite"

# Start programs
$HOME/.fehbg &
[ -z "$(pgrep sxhkd)" ] && sxhkd &
[ -z "$(pgrep udiskie)" ] && udiskie &
[ -z "$(pgrep mpd)" ] && mpd &
mpc update
eval `ssh-agent`
[ -z "$(pgrep dunst)" ] && dunst &
[ -z "$(pgrep statusbar)" ] && statusbar &
[ -z "$(pgrep nextcloud)" ] && nextcloud &
